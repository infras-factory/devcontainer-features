{
  "test-ubuntu-minimal": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "remoteUser": "vscode",
    "features": {
      "riso-bootstrap": {
        "projectName": "test-project",
        "enableSerena": false
      }
    }
  },
  "test-ubuntu-with-serena": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "remoteUser": "vscode",
    "features": {
      "riso-bootstrap": {
        "projectName": "test-project",
        "enableSerena": true,
        "isTestMode": true
      }
    }
  }
}
